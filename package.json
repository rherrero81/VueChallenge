{
    "name": "Vue-Challenges-jest",
    "description": "Vue Challenges usin ui materials and jest",
    "version": "1.0.0",
    "author": "Rherrero",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "test": "jest",
        "node-sassMaria": "node-sass --output-style compressed -o src/html/marialunarillos/scss src/html/marialunarillos/scss",
        "sass:watchMaria": "node-sass  -wo src/html/marialunarillos/scss src/html/marialunarillos/scss",
        "node-sassVanilla": "node-sass --output-style compressed -o src/html/VanillaJS/sass src/html/VanillaJS/sass",
        "sass:watchVanilla": "node-sass -wo src/html/VanillaJS/sass src/html/VanillaJS/sass"
    },
    "dependencies": {
        "open": "^7.3.0",
        "vanilla-framework": "^2.19.2",
        "vue": "latest",
        "vue-material": "latest"
    },
    "devDependencies": {
        "@vue/test-utils": "^1.0.0-beta.25",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "file-loader": "^1.1.4",
        "jest": "^21.2.1",
        "jest-serializer-vue": "^0.2.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^10.0.4",
        "vue-jest": "^2.6.0",
        "vue-loader": "^13.0.5",
        "vue-server-renderer": "latest",
        "vue-template-compiler": "latest",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.9.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ]
    }
}
